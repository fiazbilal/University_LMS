
<% current_user.favorites.each do |course| %>       
    <% tempCourse = Course.find(course.Course_id) %>
<div class="row">
   <div class="col-lg-6 mb-4">
       <div class="card h-100">
           <div class="card-body">
               <h4 class="card-title">
               <%= link_to tempCourse.course_title, course_path(tempCourse.id) %>
               </h4>
               <p>
               <%=tempCourse.course_description%>
               </p>
           </div>
       </div>
   </div>

</div>
<!-- /.row -->
<% end %>